{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "error",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "no-console": 0,
        "indent": [
            "error",
            2
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": ["error", "never"],
        "eqeqeq": ["error", "always"],
        "object-curly-newline": [
            "error",
            {
              "multiline": true,
              "consistent": false
            }
          ]
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"]
}